{
  "name": "xpress-kit",
  "version": "1.0.0",
  "description": "This is a starter kit for ExpressJS",
  "main": "app.js",
  "scripts": {
    "prestart": "node helpers/messages/startMessage",
    "start": "npm-run-all --parallel open:app lint:watch",
    "open:app": "nodemon -q app",
    "security-check": "nsp check",
    "lint": "esw .",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest --silent",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [
    "NodeJS",
    "ExpressJS"
  ],
  "author": "Shahid",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.2",
    "mongoose": "^5.0.10"
  },
  "devDependencies": {
    "chalk": "^2.3.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-watch": "^3.1.3",
    "jest": "^22.1.4",
    "nodemon": "^1.14.12",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0"
  }
}
